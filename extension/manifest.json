{
  "name": "Tab dump",
  "description": "Allow to save and load set of tabs in form of a text",
  "manifest_version": 2,
  "permissions": [
    "find",
    "tabs"
  ],
  "background": {
    "scripts": ["worker.js"]
  },
  "icons": {
    "48": "icons/icon-48.png"
  },
  "browser_action": {
    "default_title": "Tab dump",
    "default_icon": {
        "48": "icons/icon-48.png"
    },
    "default_popup": "gui/page.html"
  },
  "version": "1.0",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  }
}
