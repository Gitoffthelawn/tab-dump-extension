{
  "name": "Tab dump",
  "description": "Allows to dump to and open tabs from text list",
  "manifest_version": 2,
  "version": "2",
  "permissions": [
    "find",
    "tabs"
  ],
  "background": {
    "scripts": ["background/userDataController.js","background/tabController.js","background/messageController.js"]
  },
  "icons": {
    "48": "icons/icon-48.png"
  },
  "browser_action": {
    "default_title": "Tab dump",
    "default_icon": {
        "48": "icons/icon-48.png"
    },
    "default_popup": "gui/page.html"
  }
}
